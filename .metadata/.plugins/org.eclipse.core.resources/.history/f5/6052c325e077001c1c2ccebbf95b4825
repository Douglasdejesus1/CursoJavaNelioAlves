package application;
import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		List <String>lista= new ArrayList<>();
		
		list.add("Maria");
		list.add("Douglas");
		list.add("Flavia");
		list.add("Rodrigo");
		for(String w: lista) {
			System.out.print(w+w);
		}
	}

}
