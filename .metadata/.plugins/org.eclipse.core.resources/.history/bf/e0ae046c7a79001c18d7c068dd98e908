package entities;

public class Funcionarios {

	private Integer id;
	private String name;
	private Integer ch;
	private Double valorhora;
	public Funcionarios(Integer id, String name, Integer ch,  Double valorhora) {
		
		this.id = id;
		this.ch = ch;
		this.name = name;
		this.valorhora = valorhora;
	}
	public Integer getCh() {
		return ch;
	}
	public void setCh(Integer ch) {
		this.ch = ch;
	}
	public Double getValorhora() {
		return valorhora;
	}
	public void setValorhora(Double valorhora) {
		this.valorhora = valorhora;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Integer getId() {
		return id;
	}
	
	public void changeHorario(int mudancaCH) {
		ch+=mudancaCH;
	}
	public double salario() {
		return ch*valorhora;
	
	}
}
